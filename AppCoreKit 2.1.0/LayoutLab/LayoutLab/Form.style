                                    {   "@inherits" : [ "$gray_separator" ], "fixedWidth" : 1 },

                                    {   "@class" : "CKVerticalBoxLayout", 
                                        "layoutBoxes" : [
                                            {   "@class" : "CKLayoutFlexibleSpace" },
                                            {   "@inherits" : [  "$top_label" ],    "text" : "0" },      
