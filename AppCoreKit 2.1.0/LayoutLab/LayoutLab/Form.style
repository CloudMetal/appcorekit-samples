                                "layoutBoxes" : [
                                    { "@class" : "UIImageView", "name" : "ImageLeft" }
                                ]
                            }
                        ]
                    },
                
                    {   "@inherits" : [ "$gray_separator" ], "fixedSize" : "1 79" },
                                 
                    {   "@class" : "CKVerticalBoxLayout",
                        "layoutBoxes" : [
                            {   "@class" : "CKHorizontalBoxLayout",
                                "layoutBoxes" : [
                                    {   "@class" : "CKVerticalBoxLayout",
                                        "layoutBoxes" : [
                                            {   "@class" : "CKLayoutFlexibleSpace" },
