    "$Cell1View" : {
        "layoutBoxes" : [ 
             {  "@class" : "CKHorizontalBoxLayout", "fixedHeight" : 80, "verticalAlignment" : "CKLayoutVerticalAlignmentTop", "padding" : "0 0 1 0",
