    */
    
    "CKViewController[name=Form]" : {
        "UITableView,tableViewContainer" : {
            "backgroundView" : {
                "backgroundColor" : "clearColor" 
            },
            "backgroundColor" : "clearColor" 
        },
        "view" : {
            "backgroundColor" : "0.7 0.7 0.7 1",
            "backgroundImageContentMode" : "UIViewContentModeScaleToFill"
        },
        "CKTableViewCellController[name=first]" : {
            "cellStyle" : "CKTableViewCellStyleCustomLayout",
            "flags" : "CKItemViewFlagNone",
            "UITableViewCell" : {
                "backgroundView" : {
                    //"@inherits" : [ "$cellShadow" ],
                    "borderShadowColor" : "0 0 0 0.6",
                    "borderShadowRadius" : 6,
                    "borderShadowOffset" : "0 4",
                    
                    "separatorColor" : "0.6 0.6 0.6 1",
                    "embossTopColor" : "whiteColor",
                    "backgroundGradientLocations" : [ 0,0.6, 1 ],
                    "backgroundGradientColors" : [ "0.91 0.91 0.91 1","0.90 0.90 0.90 1", "0.81 0.81 0.81 1" ],
                    "cornerSize" : 5,
                    "borderColor" : "0.33 0.33 0.33 1",
                    "embossBottomColor" : "0.7 0.7 0.7 0.6",
                    "borderWidth" : 1
                },
                "contentView" : {
                    "@inherits" : [  "$Cell1View" ],
                    "UIImageView[name=ImageLeft]" : {
                         "image" : "Instagram_Icon_Large1",
                        "clipsToBounds" : 1,
                        "layer" : {
                            "cornerRadius" : 5
                        }
                    },
                    "UIButton[name=FollowButton]" : {
                        "defaultTitle" : "Follow",
                        "defaultTextShadowColor" : "blackColor",
                        "defaultBackgroundImage" : ["button-dark-blue", "15 0"],
                        "highlightedBackgroundImage" : ["button-dark-blue-highlight", "15 0"],
                        "titleLabel " :{
                            "shadowOffset" : "0 1",
                            "font" : ["Basic Gothic Mobi Pro Black", 12]
                        }
                    },
                    "UIView[name=ImageHighlightBackgroundView]" : {
                        "backgroundColor" : "1 1 1 1",
                        "layer" : {
                            "cornerRadius" : 10,
                            "shadowColor" : "0.3 0.3 0.3 0.3",
                            "shadowRadius" : 2,
                            "shadowOffset" : "0 1",
                            "shadowOpacity" : 1
                        }
                    }
                }
            }
        },
        
        "CKTableViewCellController[name=second]" : {
            "cellStyle" : "CKTableViewCellStyleCustomLayout",
            "flags" : "CKItemViewFlagNone",
            "UITableViewCell" : {
                "backgroundView" : {
                    //"@inherits" : [ "$cellShadow" ],
                    "borderShadowColor" : "0 0 0 0.6",
                    "borderShadowRadius" : 6,
                    "borderShadowOffset" : "0 4",
                    
                    "separatorColor" : "0.6 0.6 0.6 1",
                    "embossTopColor" : "whiteColor",
                    //"backgroundColor" : "0.91 0.91 0.91 1",
                    "backgroundGradientLocations" : [ 0,0.6, 1 ],
                    "backgroundGradientColors" : [ "0.91 0.91 0.91 1","0.90 0.90 0.90 1", "0.81 0.81 0.81 1" ],
                    "cornerSize" : 5,
                    "borderColor" : "0.33 0.33 0.33 1",
                    "embossBottomColor" : "0.7 0.7 0.7 0.6",
                    "borderWidth" : 1
                },
                "contentView" : {
                    "@inherits" : [  "$Cell2View" ]
                },
                "UIButton[name=URL]" : {
                    "defaultTitle" : "bit.ly/onthecouh01",
                    "defaultTextColor" : "0.31 0.43 0.6 1", 
                    "highlightedTextColor" : "whiteColor", 
                    "defaultShadowColor" : "whiteColor",
                    "highlightedShadowColor" : "clearColor",
                    "titleLabel" : {
                        "shadowOffset" : "0 1",
                        "font" : ["Basic Gothic Mobi Pro Black", 10],
                        "numberOfLines" : 1
                    }
                }
